{{ dump() }}

{% embed 'sb_bazo:hero' with {
  style: 'cta',
  image: 'https://images.unsplash.com/photo-1551522435-a13afa10f103?q=80&w=3870&auto=format&fit=crop&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D'
} %}
  {% block heading %}
    {% embed 'sb_bazo:heading' with {
      level: '1'
      content: content
    } only %}
      {% block heading_content %}
        {{ content.field_title}}
      {% endblock %}
    {% endembed %}
  {% endblock %}

  {% block subheading %}
    {% embed 'sb_bazo:heading' with {
      level: '2',
      classes: ['subheading'],
      content: content
     } only %}
      {% block heading_content %}
        {{ content.field_content }}
      {% endblock %}
     {% endembed %}
  {% endblock %}

  {% block cta %}
    {{ content.field_link }}
  {% endblock %}


