{% set classes = [
  'site-header'
] %}

<header {{ attributes }} class="{% for class in classes %} {{class}}{% endfor %}">
  <div{{ content_attributes }}>
    {% block region_content %}
      {% if region_content %}
        {{region_content}}
      {% else %}
        <div>Region content here.</div>
      {% endif %}
      
    {% endblock %}
  </div>
</header>